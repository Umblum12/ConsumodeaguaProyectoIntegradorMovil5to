<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Consumodeagua.Views.Graficas"
             BackgroundColor="#183592">
    <ContentPage.Content >
        <StackLayout BackgroundColor="#183592">
            <ActivityIndicator x:Name="activity" Color="Blue" BackgroundColor="#183592" HorizontalOptions="Center" VerticalOptions="Center" Scale="2" IsVisible="false" Margin="50"></ActivityIndicator>
            <WebView BackgroundColor="#183592" x:Name="webViewPage" WidthRequest="1000" HeightRequest="1000" Navigated="webViewPage_Navigated" Navigating="webViewPage_Navigating"></WebView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>